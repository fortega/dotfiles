#!/bin/sh
# Notebooks dir
N=$HOME/jupyter-notebooks
# Jupyter path
P=$HOME/.local/bin
# Spark Home
if [ -d "$HOME/spark" ]; then
    export SPARK_HOME=$HOME/spark
fi

if [ -f $P/jupyter ]; then
    if [ -d $N ]; then
	cd $N
    fi
    
    $P/jupyter notebook --ip 127.0.0.1
else
    echo "$P/jupyter not found"
fi
